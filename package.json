{
  "name": "you-hungry?",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/check-server.js 3000 && next dev --turbopack",
    "dev:force": "next dev --turbopack",
    "dev:kill": "lsof -ti:3000 | xargs kill -9",
    "build": "next build --turbopack",
    "build:webpack": "next build",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run lint && npm run test && npm run build",
    "perf:collect": "node performance-metrics/collect-metrics.js",
    "perf:compare": "node performance-metrics/compare-metrics.js",
    "perf:dashboard": "node performance-metrics/dashboard.js",
    "perf:all": "npm run perf:collect && npm run perf:dashboard",
    "logs:replace": "node scripts/replace-console-logs.js",
    "logs:clean": "npm run logs:replace && npm run lint:fix"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@clerk/nextjs": "^6.32.2",
    "@googlemaps/js-api-loader": "^2.0.1",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@googlemaps/react-wrapper": "^1.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.22",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.544.0",
    "mongodb": "^6.20.0",
    "next": "15.5.4",
    "node-fetch": "^3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "svix": "^1.76.1",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.10.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
